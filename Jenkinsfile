    agent any 
    stages {
        stage('Build') { 
            steps {
                sh "echo 'Built..'" 
            }
        }
        stage('Test') { 
            steps {
                sh "echo 'Testing..'"
            }
        }
        stage('Deploy') { 
            steps {
                sh "echo 'Deploying..'" 
            }
        }
    }
}
